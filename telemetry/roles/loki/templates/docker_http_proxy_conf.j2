[Service]
Environment="HTTP_PROXY={{ proxy[0].http_proxy | default('', true) }}"
Environment="HTTPS_PROXY={{ proxy[0].http_proxy | default('', true) }}"
Environment="NO_PROXY=localhost,{{ oim_hostname }},*.{{ oim_domain_name }},{{ oim_ip_addresses | join(',') }}{% if no_proxy_input_status %},{{ proxy[0].no_proxy }}{% endif %}"
